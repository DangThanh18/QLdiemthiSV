            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TenMT)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SoTC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NgayThi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Diem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SinhVien.TenSV)
            </td>
        </tr>
    }

</table>
<p>
    @Html.ActionLink("Back to list", "Index")
</p>
